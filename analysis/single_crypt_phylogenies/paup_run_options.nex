Run_Options.nex
#NEXUS

begin data;
dimensions ntax=?NTAX? nchar=?NCHAR?;
format datatype=standard;
matrix
?MAT?
;
end;

begin __paup;
set autoclose=yes warntree=no warnreset=no;
log start file=paup.log replace;
outgroup 1;
hsearch rearrLimit=10000000 hold=1000 start=stepwise rseed=76 nreps=20 addseq=random;
root;
filter best=yes;
savetrees file=fileref.bestall.tre brlens=yes savebootp=nodelabels format=Newick;
pscores all/TL=yes CI=yes RI=yes RC=yes HI=yes Scorefile=fileref.scores;
bootstrap nreps=20 search=heuristic keepall=yes grpfreq=yes treefile=fileref.boot.tre brlens=yes format=Newick / rearrLimit=10000000 hold=1000 start=stepwise rseed=76 nreps=20 addseq=random;
root;
savetrees file=fileref.bootMajRule.tre from=1 to=1 BrLens=yes savebootp=nodelabels format=Newick;
gettrees file=fileref.bestall.tre StoreTreeWts=yes StoreBrLens=yes mode=7 duptrees=eliminate;
filter best=yes;
contree all/strict=yes MajRule=no LE50=no usetreewts=yes treefile=fileref.strictMP.tre format=Newick;
filter best=yes;
savetrees file=fileref.unique.tre BrLens=yes savebootp=nodelabels format=Newick;
gettrees file=fileref.boot.tre StoreBrLens=yes StoreTreeWts=yes mode=3 duptrees=keep;
contree all/strict=no Percent=50 MajRule=yes LE50=no Rootmethod=outgroup usetreewts=yes outroot=monophyl treefile=fileref.contree50.tre format=Newick;
describeTrees/homoplasy=yes chgList=no mprSets=no brLens=yes apoList=no xout=both;
log stop;
end;

begin paup;
set autoclose=yes warntree=no warnreset=no maxtrees=100000 increase=auto;

log start file=paup.log replace;

log start file=paup.log replace;

outgroup 1;

bootstrap nreps=10000 search=heuristic keepall=yes grpfreq=yes treefile=fileref.boot.tre brlens=yes format=Newick cutoffPct=5  / rearrLimit=1000000000 hold=1 start=stepwise rseed=76 addseq=closest nbest=all mulTrees=yes steepest=no;

root;

savetrees file=fileref.bootMajRule.tre BrLens=yes savebootp=nodelabels format=Newick;

clearTrees;

gettrees file=fileref.bootMajRule.tre StoreTreeWts=yes StoreBrLens=yes mode=7 duptrees=eliminate;

describeTrees / brLens=yes showTaxNum=yes chgList=no apoList=yes mprSets=no xout=Both;

savetrees file=fileref.bootMajRule.withDists.tre BrLens=yes savebootp=nodelabels format=Newick;

clearTrees;

gettrees file=fileref.boot.tre StoreTreeWts=yes StoreBrLens=yes mode=7 duptrees=eliminate;

filter best=yes;

savetrees file=fileref.bestTrees.tre BrLens=yes savebootp=nodelabels format=Newick;

gettrees file=fileref.boot.tre StoreTreeWts=yes StoreBrLens=yes mode=7 duptrees=eliminate;

filter best=yes;

contree all/strict=yes MajRule=no LE50=no usetreewts=yes treefile=fileref.strictMP.tre format=Newick;

filter best=yes;

savetrees file=fileref.unique.tre BrLens=yes savebootp=nodelabels format=Newick;

gettrees file=fileref.boot.tre StoreBrLens=yes StoreTreeWts=yes mode=3 duptrees=keep;

contree all/strict=no Percent=50 MajRule=yes LE50=no Rootmethod=outgroup usetreewts=yes outroot=monophyl treefile=fileref.contree50.tre format=Newick;

describeTrees / brLens=yes homoplasy=yes showTaxNum=yes chgList=no apoList=yes mprSets=no xout=Both;

showTrees / userBrLens=yes;

